{
  "name": "golang-search",
  "displayName": "Go Search - Dependencies & Modules",
  "description": "Fast search in Go dependencies, standard library, and workspace files with background indexing",
  "version": "2.1.3",
  "publisher": "kaymon",
  "author": {
    "name": "kaymon"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h-kaymon/vscode-golang-search/"
  },
  "icon": "images/icon.png",
  "license": "MIT",
  "keywords": [
    "golang",
    "go",
    "search",
    "dependencies",
    "modules"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:**/go.mod",
    "workspaceContains:**/go.sum", 
    "workspaceContains:**/*.go",
    "onCommand:golang-search.search",
    "onCommand:golang-search.searchInView",
    "onCommand:golang-search.rebuildIndex",
    "onCommand:golang-search.showWorkspaceIndexes",
    "onCommand:golang-search.cleanupIndexes",
    "onView:golangSearchView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "golang-search.search",
        "title": "Search in Go Dependencies (Exact)",
        "icon": "$(search)"
      },
      {
        "command": "golang-search.searchFuzzy",
        "title": "Search in Go Dependencies (Fuzzy)",
        "icon": "$(search-fuzzy)"
      },
      {
        "command": "golang-search.searchInView",
        "title": "Focus Search View",
        "icon": "$(list-selection)"
      },
      {
        "command": "golang-search.rebuildIndex",
        "title": "Rebuild Go Search Index",
        "icon": "$(refresh)"
      },
      {
        "command": "golang-search.showWorkspaceIndexes",
        "title": "Go Search: Show Workspace Indexes"
      },
      {
        "command": "golang-search.cleanupIndexes",
        "title": "Go Search: Clean Up Index Files"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "golang-search",
          "title": "Go code search",
          "icon": "$(search)"
        }
      ]
    },
    "views": {
      "golang-search": [
        {
          "id": "golangSearchView",
          "name": "Go Search",
          "type": "webview"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "golangSearchView",
        "contents": "Enter keywords to search Go code and dependencies.\n[Search](command:golang-search.search)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "golang-search.search",
          "when": "view == golangSearchView",
          "group": "navigation"
        },
        {
          "command": "golang-search.rebuildIndex",
          "when": "view == golangSearchView",
          "group": "navigation"
        },
        {
          "command": "golang-search.showWorkspaceIndexes",
          "when": "view == golangSearchView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts"
  },
